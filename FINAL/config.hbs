<!DOCTYPE html>
  <html lang="en">
    <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>SELLER CONFIGURATION</title>
    <style>
      html {
        padding: 0;
        margin: 0;
     }

      body {
 
        background-color: rgb(221, 247, 149);
        position: relative;
        font-family: "Times New Roman", Times, serif;
        width: 100%;
        margin: 0;
      }
      section-2 {
        column-count: 3;

      }
    </style>
    

    </head>


    <body>
      <frameset rows="50%,50%" cols="25%,25%">
      <br>
    <form  align = "center" action="/seller" id="form1" name="form1" method="POST">

      DATABASE:<select name ="db" selected='selected' value="" style="font-size:15px;" id ="DBdropDown" onChange="this.form.submit()">
          <option>Select Database</option>
          {{#each dbs}}
             <option value="{{this.Database}}">{{this.Database}}</option>
          {{/each}} 
               
      </select>
      
      <br><br>
      
      TABLE: <select name ="table" selected='selected' style="font-size:15px;" id ="TBdropDownId"onChange="this.form.submit()">
         <option>Select Table</option>
         {{#each tb}}
           
             <option value="{{this.TABLE_NAME}}">{{this.TABLE_NAME}}</option>
         
          {{/each}} 
          
            
      </select>
      
      <br><br>
      
      ATTRIBUTES: <select name ="attribute" value="this.state.value" style="font-size:15px;" id ="ColdropDownId" onChange="this.form.submit()">
           <option>Select Attributes</option>
           {{#each col}}
           
             <option value="{{this.COLUMN_NAME}}">{{this.COLUMN_NAME}}</option>
         
          {{/each}} 
            
      </select>
      <br><br>
      <button style="font-size:15px" value="config" type ="submit" formaction="/configView">SAVE</button>
    </form>

    
       </center>
     </body>
     </frameset>
   </html>